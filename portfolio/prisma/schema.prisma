generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model Project {
  id            Int       @id @default(autoincrement())
  name          String    @unique @db.VarChar(55)
  description   Json? 
  link          String?   @db.Text
  technologies  String    @db.Text
  creationDate  DateTime  @db.Date
  havePage      Boolean
  page          String?   @db.Text
  label         String    @db.Text
  langs         String    @db.Text
}